{
  "manifest_version": 3,
  "name": "__MSG_name__",
  "description": "__MSG_desc__",
  "default_locale": "en",
  "background": { "service_worker": "background.bundle.js" },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "https://sshot.org/*",
    "contextMenus",
    "chrome://favicon/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "icon-128.png", "icon-34.png"],
      "matches": []
    }
  ]
}
